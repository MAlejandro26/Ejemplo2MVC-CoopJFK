            text: "Esta acción no se puede Revertir!",
            type: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Si, Cerrar!'
        }).then((result) => {
            if (result.value) {

                var data = {
                    ID: ID, 
                    FECHA_APERTURA: get('fechatransact')

                }
                $.ajax({
                    type: "Post",
                    url: "/Arqueo/CerrarArqueo",
                    data: data,
                    success: function (result) {
                        if (result.result == true) {
                            window.location.href = "/Arqueo/Arqueo?transaccion=" + get('transaccion') + '&fechatransact=' + get('fechatransact');
                            // $("#MyModal").modal("hide");
                            Swal.fire(
                                'Cerrado!',
                                'El arqueo ha sido cerrado, ya no se podra modificar!',
                                'success'
                            )
                        }
                        else {
                            Swal.fire({
                                type: 'error',
                                title: 'Oops...',
                                text: 'El arqueo ya esta cerrado, no lo puede modificar',
