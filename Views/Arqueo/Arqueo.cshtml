    function get(name) {
        if (name = (new RegExp('[?&]' + encodeURIComponent(name) + '=([^&]*)')).exec(location.search))
            return decodeURIComponent(name[1]);
    }



    function EditArqueoRecord(ID) {
        Swal.mixin({
            input: 'text',
            confirmButtonText: 'Guardar',
            showCancelButton: true,
            progressSteps: ['1']
        }).queue([
            {
                title: 'Inicio',
                text: 'Ingrese el Inicio de esta denominación en el arqueo'
            }

        ]).then((result) => {
            if (result.value) {
                var data = {
                    ID: ID,
                    INICIO: result.value
                }
                $.ajax({
                    type: "Post",
                    url: "/Arqueo/SaveDataInDatabase",
                    data: data,
                    success: function (result) {

                        window.location.href = "/Arqueo/Arqueo?transaccion=" + get('transaccion') + '&fechatransact=' + get('fechatransact');
