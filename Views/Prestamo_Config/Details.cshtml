@model CoopJFK.Models.PRESTAMO_CONFIG

@{
    ViewBag.Title = "Detalle";
}

<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">@ViewBag.Title</h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a href="#">Sistema de Gestión de Crédito</a></li>
            <li>Administración</li>
            <li class="active">Configuración de Prestamos</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-md-12">
        <div class="white-box">
            <div>
                <h4>Configuración de Prestamos</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayName("Tipo de Prestamo")
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.TIPO_PRESTAMO1.DESCRIPCION)
                    </dd>

                    <dt>
                        Antiguedad Minima
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ANTIGUEDAD_MINIMA)
                    </dd>

                    <dt>
                        Antiguedad Maxima
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ANTIGUEDAD_MAXIMA)
                    </dd>

                    <dt>
                        Plazo Minimo
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.PLAZO_MINIMO)
                    </dd>

                    <dt>
                        Plazo Maximo
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.PLAZO_MAXIMO)
                    </dd>

                    <dt>
                        Interes
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.INTERES)
                    </dd>

                    <dt>
                        Plazo Capitalización
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.PLAZO_CAPITALIZACION)
                    </dd>

                    <dt>
                        Plazo Retorno
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.PLAZO_RETORNO)
                    </dd>

                    <dt>
                        @Html.DisplayName("Usuario de Creación")
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.USUARIO_CREACION)
                    </dd>
                    <dt>
                        @Html.DisplayName("Fecha de Creación")
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.FECHA_CREACION)
                    </dd>


                    <dt>
                        @Html.DisplayName("Usuario de Modificación")
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.USUARIO_MODIFICACION)
                    </dd>
                    <dt>
                        @Html.DisplayName("Fecha de Modificación")
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.FECHA_MODIFICACION)
                    </dd>

                </dl>
            </div>
            <p>
                @Html.ActionLink("Editar", "Edit", new { id = Model.ID }) |
                @Html.ActionLink("Atrás", "Index")
            </p>

        </div>
    </div>
</div>


            