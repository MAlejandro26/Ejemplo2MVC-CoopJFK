                                enabled: true
                            },
                            showBorders: true,
                            columns: [
                                {
                                    dataField: "ID",
                                    caption: "No. Prestamo",
                                    alignment: "center"
                                },
                                {
                                    dataField: "TIPO",
                                    caption: "Tipo de Ptmo.",
                                    alignment: "center"
                                },
                                {
                                    dataField: "MONTO",
                                    caption: "Monto Ptmo.",
                                    dataType: "number",
                                    alignment: "center"
                                },
                                {
                                    dataField: "SALDO",
                                    caption: "Saldo Ptmo.",
                                    dataType: "number",
                                    alignment: "center"
                                },
                                {
                                    dataField: "CUOTA",
                                    caption: "Cuota Por PP",
                                    alignment: "center"
                                },
                                {
                                    dataField: "PLAZO",
                                    caption: "Plazo Ptmo.",
                                    alignment: "center"
                                },
                                {
                                    dataField: "CUOTAS_PAGADAS",
                                    caption: "Cuotas Pagadas",
                                    alignment: "center"
                                },
                                {
                                    dataField: "CUOTAS_PENDIENTES",
                                    caption: "CuotaS P.",
                                    alignment: "center"
                                },
                                {
                                    dataField: "PPFIN",
                                    caption: "P. de fin",
                                    alignment: "center"
                                },
                                {
                                    dataField: "FECHA",
                                    caption: "Fecha Fin",
                                    dataType: "date",
                                    alignment: "center"
                                }

                            ],

                        }).dxDataGrid("instance");
                    }, error: function (xhr, status, error) {
                        alert(status);
                    }
                });

            };

