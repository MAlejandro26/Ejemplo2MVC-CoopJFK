@model CoopJFK.Models.ViewModels.Consulta_CreditoViewModels

@{
    ViewBag.Title = "Anlisis de Crédito";
}
<link href="~/Content/dx.common.css" rel="stylesheet" />
<link href="~/Content/dx.light.css" rel="stylesheet" />
@*<script src="~/Scripts/jquery-3.4.1.min.js"></script>*@

<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title"></h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a href="#">Sistema de Gestión de Crédito</a></li>
            <li>Préstamo</li>
            <li class="active">Analisis de Crédito</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="white-box">
                <h4>SITUACION FINANCIERA DEL SOCIO</h4>


                <hr />

                <div class="form-horizontal">
                    <h5>DATOS GENERALES DEL SOCIO</h5>
                    <hr />

                    <div class="form-group">
                        <div class="col-md-2">
                            @Html.Label("No. Socio")
                            @Html.TextBoxFor(model => model.Socio, new { @class = "form-control", placeholder = "Aqui #socio" })
                            <span class="error">El socio es requerido</span>
                        </div>
                        <div class="col-md-4">
                            @Html.Label("Nombres y Apellidos")
                            @Html.TextBoxFor(model => model.Nombre_Socio, new { @class = "form-control", @readonly = "true" })
                        </div>

                        <div class="col-md-3">
                            @Html.Label("No.Empleado")
                            @Html.TextBoxFor(model => model.Empleado, new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Agencia")
                            @Html.TextBoxFor(model => model.DescAgencia, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Periodo de Ingreso")
                            @Html.TextBoxFor(model => model.Periodo_Ingreso, new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Fecha de Ingreso")
                            @Html.TextBoxFor(model => model.Fecha_Ingreso, new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Actualizado al PP")
                            @Html.TextBoxFor(model => model.DescPeriodo, new { @class = "form-control", @readonly = "true" })
                            @*@Html.DropDownList("periodo", new SelectList(string.Empty, "Value", "Text"), "--Seleccione--", new { @class = "form-control" })*@
                            <span class="error">El periodo es obligatorio</span>
                        </div>
                        <div class="col-md-3">
                            @Html.Label("Salario por pago")
                            @Html.TextBoxFor(model => model.Salario, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Antiguedad del socio")
                            @Html.TextBoxFor(model => model.Antiguedad, new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Derecho")
                            @Html.TextBoxFor(model => model.Derecho, new { @class = "form-control", @readonly = "true" })

                            <span class="error">La equivalencia es requerida</span>

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Ahorro extraordinario")
                            @Html.TextBoxFor(model => model.Antiguedad, new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>

                    <h5>DATOS DE AHORRO Y PRESTAMOS</h5>
                    <hr />

