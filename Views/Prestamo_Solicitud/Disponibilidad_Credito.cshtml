                    </div>
                    <div class="form-group">
                    </div>
                    <div class="form-group">
                    </div>
                    <div class="form-group">
                    </div>
                    <div class="form-group">
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Capacidad de Pago")
                            @Html.TextBoxFor(model => model.Capacidad_Pago, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Derecho a Ptamo x Capacidad de Pago")
                            @Html.TextBoxFor(model => model.Derecho_Montoxcapacidad, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Cuotas x Capacidad de Pago")
                            @Html.TextBoxFor(model => model.cuotasxCapacidad, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>
                    <hr />
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Tipo de Prestamo")
                            @Html.DropDownList("TipoPrestamo", null, "--Seleccione--", htmlAttributes: new { @class = "form-control" })
                            <span class="error">El Tipo de prestamo es requerido</span>

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Plazo préstamo")
                            @Html.TextBoxFor(model => model.Plazo, new { @class = "form-control" })
                            <span class="error">El plazo del prestamo es requerido</span>
                        </div>
                        <div class="col-md-3">
                            @Html.Label("Monto Solicitado")
                            @Html.TextBoxFor(model => model.Monto_Solicitado, "{0:n2}", new { @class = "form-control" })
                            <span class="error">El monto soliictado es requerido</span>
                        </div>

                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            <i class="fa fa-calculator"></i> <input id="calcular" type="button" value="Calcular" class="btn btn-success btn-lg" style="padding:5px 15px" />
                        </div>
                    </div>

                    <hr />
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Principal Préstamo")
                            @Html.TextBoxFor(model => model.Principal, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Interés préstamo")
                            @Html.TextBoxFor(model => model.Interes, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Cuota")
                            @Html.TextBoxFor(model => model.Cuota, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Principal Cuota")
                            @Html.TextBoxFor(model => model.Principal_Cuota, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Interés Cuota")
                            @Html.TextBoxFor(model => model.Interes_Cuota, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Plazo inicia en Periodo de pago (Fecha)")
                            @Html.TextBoxFor(model => model.Fecha_Inicio, "{0:d}", new { @class = "form-control", @readonly = "true" })

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Plazo finaliza en Periodo de pago (Fecha)")
                            @Html.TextBoxFor(model => model.Fecha_Fin, "{0:d}", new { @class = "form-control", @readonly = "true" })

