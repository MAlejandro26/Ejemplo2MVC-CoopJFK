                        </div>
                        <div class="col-md-3">
                            @Html.Label("Cuota de Aportacion por PP")
                            @Html.TextBoxFor(model => model.Cuota_Aportacion, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Saldo de ahorro")
                            @Html.TextBoxFor(model => model.Saldo_Ahorro, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Saldo de Aportacion")
                            @Html.TextBoxFor(model => model.Saldo_Aportacion, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>



                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.Label("Saldo en Deuda")
                            @Html.TextBoxFor(model => model.Saldo_Deuda, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Porcentaje de endeudamiento")
                            @Html.TextBoxFor(model => model.Porcentaje_Endeudamiento, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Balance de ahorro versus prestamo")
                            @Html.TextBoxFor(model => model.Balance, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>
                        <div class="col-md-3">
                            @Html.Label("Derecho a Prestamo")
                            @Html.TextBoxFor(model => model.Derecho_Monto, "{0:n2}", new { @class = "form-control", @readonly = "true" })

                        </div>

                    </div>
                    <h5>PRESTAMOS ACTIVOS</h5>
                    <div id="gridPrestamos"></div>
                    <hr />

                    <h5>OTROS</h5>
                    <hr />
