                    $("#monto_cuotas_aportacion").prop('disabled', true);
                   
                }
                else {
                    $("#cant_cuotas_aportacion").prop('disabled', true);
                    $("#monto_cuotas_aportacion").prop('disabled', true);
                   
                }


            }
            var data = {
                aportacion: $("#aportacion").val().trim(),
                paga: $("#Pago_aportacion").val().trim()
            }

            $.ajax({
                url: '/Caja/GetDatoAportacion',
                type: "POST",
                data: JSON.stringify(data),
                dataType: "JSON",
                contentType: "application/json",
                success: function (d) {
                    if (d.status == true) {

                        if (tipo == 'AB') {
                            $("#monto_cuotas_aportacion").val(d.monto_cuota);
                            $("#cant_cuotas_aportacion").val(0);
                           total_cuotas_aportacion = d.cant_cuotas;
                        }
                        else {
                            $("#monto_cuotas_aportacion").val(d.monto_cuota);
                            $("#cant_cuotas_aportacion").val(d.cant_cuotas);
                        }
                    }
                    else {

                        Swal.fire({
                            type: 'error',
                            title: 'Error!',
                            text: d.messageerror,
                            footer: ''
                        })

                    }

                },
                error: function () {
                    Swal.fire({
                        type: 'error',
                        title: 'Error!',
                        text: 'Error al obtener los montos de las aportaciones',
                        footer: ''
                    })

                }
            })
        })//fin pagos aportacion

            $('#prestamo').change(function () {
                $("#cant_cuotas").val('');
                $("#monto_cuotas").val('');
                $("#total_monto_cuotas").val('');
                $("#Pago").val('0');

            })

            $('#cant_cuotas').change(function () {
                var cantidad = $("#cant_cuotas").val().trim()
                var valor_cuota = $("#monto_cuotas").val().trim()
                if (cantidad > total_cuotas) {
                    Swal.fire({
                        type: 'error',
                        title: 'Error!',
                        text: 'Error las cuotas digitadas no pueden superar el total de cuotas del préstamo',
                        footer: ''
                    });
                    $("#cant_cuotas").val('0');
                }
                else
                {
                    var t = cantidad * valor_cuota;
                    $("#total_monto_cuotas").val(t.toFixed(2));
                }
                

            })

        $('#cant_cuotas_aportacion').change(function () {
            var cantidad = $("#cant_cuotas_aportacion").val().trim()
            var valor_cuota = $("#monto_cuotas_aportacion").val().trim()
                if (cantidad > total_cuotas_aportacion) {
                    Swal.fire({
                        type: 'error',
                        title: 'Error!',
                        text: 'Error las cuotas digitadas no pueden superar el total de cuotas de las aportaciones',
                        footer: ''
                    });
                    $("#cant_cuotas_aportacion").val('0');
                }
                else {
                    var data = {
                        aportacion: $("#aportacion").val().trim(),
                        paga: $("#Pago_aportacion").val().trim(),
                        cuotas: cantidad
                    }

                    $.ajax({
                        url: '/Caja/GetMontoAportacion',
                        type: "POST",
                        data: JSON.stringify(data),
                        dataType: "JSON",
                        contentType: "application/json",
                        success: function (d) {
                            if (d.status == true) {
                    
                             $("#monto_cuotas_aportacion").val(d.monto_cuota);
                                               
                            }
                            else {

                                Swal.fire({
                                    type: 'error',
                                    title: 'Error!',
                                    text: d.messageerror,
                                    footer: ''
                                })

                            }

                        },
                        error: function () {
                            Swal.fire({
                                type: 'error',
                                title: 'Error!',
                                text: 'Error al obtener los montos de las aportaciones',
                                footer: ''
                            })

                        }
                    })
                }


            })

        $('#socio_ocurrencia').change(function () {
            var isValidAyuda=true;


            if ($('#periodesc_funeral').val().trim() == '') {
                Swal.fire({
                    type: 'error',
                    title: 'Error!',
                    text: 'Debe digitar el periodo de la ayuda',
                    footer: ''
                })
                isValidAyuda = false;
            }
            if (isValidAyuda) {
                var data = {
                    socio: $("#socio_ocurrencia").val().trim(),
                    Periodo: $("#periodesc_funeral").val().trim()
               
                }
                $.ajax({
                    url: '/Caja/GetAyudaFuneral_Retirar',
                    type: "POST",
                    data: JSON.stringify(data),
                    dataType: "JSON",
                    contentType: "application/json",
                    success: function (d) {
                        if (d.status == true) {
                            
                            $("#monto_funeral").val(d.monto_funeral);
                            $("#nombre_fallecido").val(d.nombre_fallecido);
                            $("#fecha_ocurrencia").val(d.fecha_ocurrencia);
                            $("#PARENTESCO").val(d.parentesco).change();

                        }
                        else {

                            Swal.fire({
                                type: 'error',
                                title: 'Error!',
                                text: d.messageerror,
                                footer: ''
                            })

                        }

                    },
                    error: function () {
                        Swal.fire({
                            type: 'error',
                            title: 'Error!',
                            text: 'Error al obtener los datos de la ayuda funeral',
                            footer: ''
                        })

                    }
                })
            }

        })
       


    </script>
