@model CoopJFK.Models.ViewModels.CajaViewModel

@{
    ViewBag.Title = "Crear";
}

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/gijgo/combined/gijgo.min.js"></script>
<link href="~/Content/gijgo/combined/gijgo.min.css" rel="stylesheet" />

<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">@ViewBag.Title</h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a href="#">Sistema de Gestión de Crédito</a></li>
            <li>Caja</li>
            <li class="active">Crear Transacción de Retiro</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="white-box">
                <div class="master">
                    <div class="form-horizontal">
                        <h4>Retiro</h4>
                        <hr />
                        @using (Html.BeginForm())
                        {
                            <div class="form-group">
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.transaccion)
                                    @Html.DropDownList("TRANSACCION", null, "--Seleccione--", htmlAttributes: new { @class = "form-control" })

                                </div>
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.tc)
                                    @Html.EditorFor(model => model.tc, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                </div>
                                <div class="col-md-3">
                                    @Html.Label("Forma de Pago")
                                    @{
                                        List<SelectListItem> _formapago = new List<SelectListItem>();
                                        _formapago.Add(new SelectListItem { Text = "Efectivo", Value = "E" });
                                        _formapago.Add(new SelectListItem { Text = "Depósito", Value = "D" });
                                        _formapago.Add(new SelectListItem { Text = "Aplicar Ahorros", Value = "AA" });

                                    }
                                    @Html.DropDownListFor(model => model.FormaPago, _formapago, "--Seleccione--", new { @class = "form-control" })
                                    <span class="error">La forma de pago  es Obligatorio</span>
                                </div>
                                <div class="col-md-3">

                                </div>
                                <div class="col-md-3"> </div>
                            </div>
                            <div class="form-group" id="dato_socio">
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.socio)
                                    @Html.EditorFor(model => model.socio, new { htmlAttributes = new { @id = "socio", @class = "form-control", @readonly = "true" } })

                                </div>
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.empleado)
                                    @Html.Editor("empleado", new { htmlAttributes = new { @id = "empleado", @class = "form-control", @readonly = "true" } })

                                </div>
                                <div class="col-md-6">
                                    @Html.Label("Nombres y Apellidos")
                                    @Html.TextBoxFor(model => model.nombres, new { @class = "form-control", placeholder = "Escriba el nombre de un socio", id = "nombres" })

                                </div>
                            </div>
                            <div class="form-group" id="detalle_prestamo">
                                <div class="col-md-3">
                                    @Html.Label("No. Préstamo")

                                    @Html.DropDownList("prestamo", new SelectList(string.Empty, "Value", "Text"), "--Seleccione--", new { @class = "form-control" })
                                </div>
                                <div class="col-md-3">
                                    @Html.Label("Tipo de Pago")
                                    @Html.DropDownList("Pago", new SelectList(string.Empty, "Value", "Text"), "--Seleccione--", new { @class = "form-control" })
                                    <span class="error">El tipo  es Obligatorio</span>
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.cant_cuotas)
                                    @Html.EditorFor(model => model.cant_cuotas, new { htmlAttributes = new { @class = "form-control" } })
                                    <span class="error">Ingrese la Cantidad de cuotas</span>
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.monto_cuotas)
                                    @Html.EditorFor(model => model.monto_cuotas, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                    <span class="error">El monto es Obligatorio</span>
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.total_monto_cuotas)
                                    @Html.EditorFor(model => model.total_monto_cuotas, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                    <span class="error">El total es Obligatorio</span>
                                </div>
                            </div>
                            <div class="form-group" id="detalle_aportacion">

                                <div class="col-md-3">
                                    @Html.Label("No. Aportacion")

                                    @Html.EditorFor(model => model.aportacion, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                </div>
                                <div class="col-md-3">
                                    @Html.Label("Tipo de Pago")
                                    @Html.DropDownList("Pago_aportacion", new SelectList(string.Empty, "Value", "Text"), "--Seleccione--", new { @class = "form-control" })
                                    <span class="error">El tipo  es Obligatorio</span>
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.cant_cuotas_aportacion)
                                    @Html.EditorFor(model => model.cant_cuotas_aportacion, new { htmlAttributes = new { @class = "form-control" } })
                                    <span class="error">Ingrese la Cantidad de cuotas</span>
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.monto_cuotas_aportacion)
                                    @Html.EditorFor(model => model.monto_cuotas_aportacion, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                    <span class="error">El monto es Obligatorio</span>
                                </div>

                            </div>
                            <div class="form-group" id="dato_deposito">
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.minuta)
                                    @Html.EditorFor(model => model.minuta, new { htmlAttributes = new { @class = "form-control" } })

                                </div>
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.fecha_minuta)
                                    <input type="text" id="fecha_minuta" />
                                    <script>
                                        var hoy = new Date().toLocaleTimeString();
                                        $('#fecha_minuta').datepicker({
                                            uiLibrary: 'bootstrap', format: 'dd/mm/yyyy'
                                        });
                                    </script>

                                </div>
                                <div class="col-md-6">
                                    @Html.LabelFor(model => model.concepto)
                                    @Html.EditorFor(model => model.concepto, new { htmlAttributes = new { @class = "form-control" } })

                                </div>
                            </div>
                            <div class="form-group">


                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.monto_cordoba)
                                    @Html.EditorFor(model => model.monto_cordoba, new { htmlAttributes = new { @class = "form-control" } })

                                </div>
                                <div class="col-md-3">
                                    @Html.LabelFor(model => model.monto_dolar)
                                    @Html.EditorFor(model => model.monto_dolar, new { htmlAttributes = new { @class = "form-control" } })

                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.cambio)
                                    @Html.EditorFor(model => model.cambio, new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                </div>
                                <div class="col-md-1">
                                    @Html.Label("Moneda")
                                    @Html.Editor("Simbolo", new { htmlAttributes = new { @class = "form-control", @readonly = true } })
                                </div>

                            </div>

                            <div class="form-group" id="div_fallecidomontos">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.periodesc_funeral)
                                    @Html.EditorFor(model => model.periodesc_funeral, new { htmlAttributes = new { @class = "form-control" } })

                                </div>
                                <div class="col-md-6">
                                    @Html.LabelFor(model => model.socio_ocurrencia)
                                    @Html.DropDownList("socio_ocurrencia", null, "--Seleccione--", htmlAttributes: new { @class = "form-control"})
                                  
                                </div>
                                
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.monto_funeral,"Ayuda")
                                    @Html.EditorFor(model => model.monto_funeral, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })

                                </div>
                            </div>

                                <div class="form-group" id="div_fallecido">
                                    
                                    <div class="col-md-6">
                                        @Html.LabelFor(model => model.nombre_fallecido)
                                        @Html.EditorFor(model => model.nombre_fallecido, new { htmlAttributes = new { @class = "form-control", @readonly="true" } })

                                    </div>
                                    <div class="col-md-3">
                                        @Html.LabelFor(model => model.parentesco)
                                        @Html.DropDownList("PARENTESCO", null, "--Seleccione--", htmlAttributes: new { @class = "form-control", @readonly = "true" })

                                    </div>
                                    <div class="col-md-3">
                                        @Html.LabelFor(model => model.fecha_ocurrencia)
                                        <input type="text" id="fecha_ocurrencia" />
                                        <script>
                                            var hoy = new Date().toLocaleTimeString();
                                            $('#fecha_ocurrencia').datepicker({
                                                uiLibrary: 'bootstrap', format: 'dd/mm/yyyy'
                                            });
                                        </script>

                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-offset-8 col-md-10">
                                        <input id="calcular" type="button" value="Calcular" class="btn btn-success btn-lg" style="padding:5px 15px" />    |    <input id="submit" type="button" value="Procesar" style="padding:5px 15px" class="btn btn-info btn-lg" />
                                    </div>
                                </div>
                                }
                            </div>
                        </div>
            </div>
        </div>
    </div>

    <div class="row" id="efectivo">
        <div class="col-md-6">
            <div class="white-box">
                <div class="details" width="50%">
                    <h4>Detalle Denominaciones Recibidas</h4>
                    <hr />
                    <table class="space" width="50%">
                        <tr>
                            <td colspan="3">Denominación</td>
                            <td>Cantidad</td>
                            <td>&nbsp;</td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                @Html.DropDownList("denominacion_recibida", null, "--Seleccione--", htmlAttributes: new { @class = "form-control" })
                                <span class="error">Tipo beneficiario es requerido</span>
                            </td>
                            <td>
                                @Html.Editor("cantidad_recibida", new { htmlAttributes = new { @class = "form-control" } })
                                @*<input type="text" id="nombre" />*@
                                <span class="error">La Cantidad es requerida</span>
                            </td>
                            <td>
                                <input type="button" id="Agregar_recibida" value="Agregar" class="btn btn-success btn-sm" />
                            </td>
                        </tr>
                    </table>
                    <div id="deno_recibida" class="tablecontainer"></div>
                </div>

            </div>
        </div>
        <div class="col-md-6">
            <div class="white-box">
                <div class="details" width="50%">
                    <h4>Detalle Denominaciones Entregadas</h4>
                    <hr />
                    <table class="space" width="50%">
                        <tr>
                            <td colspan="3">Denominación</td>
                            <td>Cantidad</td>
                            <td>&nbsp;</td>

                        </tr>
                        <tr>
                            <td colspan="3">
                                @Html.DropDownList("denominacion", null, "--Seleccione--", htmlAttributes: new { @class = "form-control" })
                                <span class="error">Tipo beneficiario es requerido</span>
                            </td>
                            <td>
                                @Html.Editor("cantidad_entregada", new { htmlAttributes = new { @class = "form-control" } })
                                @*<input type="text" id="nombre" />*@
                                <span class="error">La Cantidad es requerida</span>
                            </td>
                            <td>
                                <input type="button" id="Agregar_entregada" value="Agregar" class="btn btn-success btn-sm"/>
                            </td>
                        </tr>
                    </table>
                    <div id="deno_entregada" class="tablecontainer"></div>
                </div>
            </div>
        </div>
    </div>



</div>




<div>
    @Html.ActionLink("Back to List", "Index")
</div>
<link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />

<link href="~/Content/DataTables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
<script src="~/Scripts/DataTables/dataTables.bootstrap.min.js"></script>
@section Scripts{
    <script src="~/Scripts/jquery-ui.js"></script>
   
    <script>
        $('#div_fallecido').hide();
        $('#div_fallecidomontos').hide();
       $('#dato_deposito').hide();
        $('#cuotas').hide();
        $('#detalle_prestamo').hide();
        $('#detalle_aportacion').hide();
        $('#efectivo').hide();
        $('#dato_socio').hide();
        var total_cuotas;
        var total_cuotas_aportacion;
        var monto_ahorro;
        $(document).ready(function () {
          
            var valor = $("#TRANSACCION").val();
           $("#nombres").autocomplete({
                dataType: 'JSON',
                source: function (request, response) {
                    jQuery.ajax({
                        url: "/Caja/BuscarSocio",
                        type: "post",
                        dataType: "json",
                        data: {
                            nombre: request.term
                        },
                        success: function (data) {
                            response($.map(data, function (item) {
                                return {
                                    id: item.Id,
                                    value: item.Name,
                                    cedula: item.Cedula,
                                    empleado: item.Empleado,
                                    cuota: item.Cuota,
                                    salario: item.Salario,
                                    tipo_pago: item.Tipo_Pago,
                                    aportaciones:item.aportacion
                                }
                            }))
                        }
                    })
                },
                select: function (e, ui) {
                    $("#socio").val(ui.item.id);
                    $("#empleado").val(ui.item.empleado);
                    $("#aportacion").val(ui.item.aportaciones);
                    $("#prestamo").empty();

                    
                    if ($("#TRANSACCION").val() == 7) { $("#monto_dolar").val(ui.item.cuota); }
                    $.ajax({

                             type: 'POST',
                             url: '@Url.Action("GetPrestamosCaja")', 
                             dataType: 'json',
                         data: { socio: ui.item.id },

                             success: function (states) {
                                 $("#prestamo").append('<option value="0">--Seleccione--</option>');
                                 $.each(states, function (i, state) {

                                $("#prestamo").append('<option value="' + state.Value + '">' +
                                     state.Text + '</option>');
                                
                                });
                            },
                            error: function (ex) {
                                alert('Failed to retrieve states.' + ex);
                            }
                    });

                    if ($("#TRANSACCION").val() == 4) { }

                }
            })
        })//fin funtion
                $(document).ready(function () {
                    var desc_recibido;
                    var desc_entregado;
                    var Denominacion = [];
                    var Denominacion_Rec = [];
                   
                    $('#denominacion').change(function () {
                        desc_entregado = $(this).find('option:selected').text();

                    });
                    $('#denominacion_recibida').change(function () {
                        desc_recibido = $(this).find('option:selected').text();

                    });
                  
                    $('#Agregar_recibida').click(function () {
                       
                        var isValidItem = true;

                        if ($('#denominacion_recibida').val().trim() == '') {
                            isValidItem = false;
                            $('#denominacion_recibida').siblings('span.error').css('visibility', 'visible');
                        }
                        else {
                            $('#denominacion_recibida').siblings('span.error').css('visibility', 'hidden');
                        }

                        if ($('#cantidad_recibida').val().trim() == '') {
                            isValidItem = false;
                            $('#cantidad_recibida').siblings('span.error').css('visibility', 'visible');
                        }
                        else {
                            $('#cantidad_recibida').siblings('span.error').css('visibility', 'hidden');
                        }

                        //Add item to list if valid
                        if (isValidItem) {
                            Denominacion.push({
                                denominacion: $('#denominacion_recibida').val().trim(),
                                cantidad: $('#cantidad_recibida').val().trim(),
                                descripcion: desc_recibido.trim(),
                                RECIBIDA: 'S',
                                ENTREGADA: 'N'
                            });

                            //Clear fields
                            $('#denominacion_recibida,#cantidad_recibida').val('');

                        }
                        //populate order items
                        GeneratedItemsTable();

                    });

                    function GeneratedItemsTable() {
                        if (Denominacion.length > 0) {
                            var $table = $('<table/>');
                            $table.append('<thead><tr><th>No</th><th>Denominación</th><th>Cantidad</th><th></th></tr></thead>');
                            var $tbody = $('<tbody/>');
                            $.each(Denominacion, function (i, val) {
                                var $row = $('<tr/>');
                                $row.append($('<td/>').html(val.denominacion));
                                $row.append($('<td/>').html(val.descripcion));
                                $row.append($('<td/>').html(val.cantidad));
                                var $remove = $('<a href="#">Quitar</a>');
                                $remove.click(function (e) {
                                    e.preventDefault();
                                    Denominacion.splice(i, 1);
                                    GeneratedItemsTable();
                                });
                                $row.append($('<td/>').html($remove));
                                $tbody.append($row);
                            });
                            console.log("current", Denominacion);
                            $table.append($tbody);
                            $('#deno_recibida').html($table);
                        }
                        else {
                            $('#deno_recibida').html('');
                        }
                    }



                    //Add button click function
                    $('#Agregar_entregada').click(function () {
                        //Check validation of order item
                        var isValidItem = true;

                        if ($('#denominacion').val().trim() == '') {
                            isValidItem = false;
                            $('#denominacion').siblings('span.error').css('visibility', 'visible');
                        }
                        else {
                            $('#denominacion').siblings('span.error').css('visibility', 'hidden');
                        }

                        if ($('#cantidad_entregada').val().trim() == '') {
                            isValidItem = false;
                            $('#cantidad_entregada').siblings('span.error').css('visibility', 'visible');
                        }
                        else {
                            $('#cantidad_entregada').siblings('span.error').css('visibility', 'hidden');
                        }

                        //Add item to list if valid
                        if (isValidItem) {
                            Denominacion_Rec.push({
                                denominacion: $('#denominacion').val().trim(),
                                cantidad: $('#cantidad_entregada').val().trim(),
                                descripcion: desc_entregado.trim(),
                                RECIBIDA: 'N',
                                ENTREGADA:'S'
                            });

                            //Clear fields
                            $('#denominacion,#cantidad_entregada').val('');

                        }
                        //populate order items
                        GeneratedItemsTableEntregado();

                    });

                    function GeneratedItemsTableEntregado() {
                        if (Denominacion_Rec.length > 0) {
                            var $table = $('<table/>');
                            $table.append('<thead><tr><th>No</th><th>Denominación</th><th>Cantidad</th><th></th></tr></thead>');
                            var $tbody = $('<tbody/>');
                            $.each(Denominacion_Rec, function (i, val) {
                                var $row = $('<tr/>');
                                $row.append($('<td/>').html(val.denominacion));
                                $row.append($('<td/>').html(val.descripcion));
                                $row.append($('<td/>').html(val.cantidad));
                                var $remove = $('<a href="#">Quitar</a>');
                                $remove.click(function (e) {
                                    e.preventDefault();
                                    Denominacion_Rec.splice(i, 1);
                                    GeneratedItemsTableEntregado();
                                });
                                $row.append($('<td/>').html($remove));
                                $tbody.append($row);
                            });
                            console.log("current", Denominacion_Rec);
                            $table.append($tbody);
                            $('#deno_entregada').html($table);
                        }
                        else {
                            $('#deno_entregada').html('');
                        }
                    }

                    $('#FormaPago').change(function () {
                        var text = $(this).find('option:selected').text();
                             var valor = $("#FormaPago").val();
                                       
                        if (valor == 'E') {
                            $('#dato_deposito').hide(); 
                            $('#efectivo').show();
                        }
                        //compra de divisas
                        else if (valor == 'D') {
                            $('#dato_deposito').show();
                            $('#efectivo').hide();
                        }
                        //venta de divisas
                        else if (valor == 'AA') {
                            $('#dato_deposito').hide();
                            $('#efectivo').hide();
                        }
                    });


                    //funcion para cambiar transaccion
                    $('#TRANSACCION').change(function () {
                        var text = $(this).find('option:selected').text();
                        var valor = $("#TRANSACCION").val();
                        clear();
                        $("#prestamo").empty();
                        //fondo ayuda funeral
                        if (valor == 4) {
                            GetTC(valor, 'V');
                            $('#div_fallecido').show();
                            $('#div_fallecidomontos').show();
                            $('#dato_deposito').hide();
                            $('#dato_socio').show();
                            $("#monto_cordoba").removeAttr("disabled");
                            $("#monto_dolar").prop('disabled', true);
                            $("#prestamo").prop('disabled', true);
                            $('#detalle_prestamo').hide();
                            $('#cuotas').hide();
                            $('#detalle_aportacion').hide();
                            $("#monto_dolar").val("0");
                            $("#cambio").val("0");
                        }
                        //compra de divisas
                        else if (valor == 1) {
                            GetTC(valor, 'C');
                            $("#monto_cordoba").prop('disabled', true);
                            $('#dato_deposito').hide();
                            $("#monto_cordoba").val("0");
                            $("#prestamo").prop('disabled', true);
                            $('#detalle_prestamo').hide();
                            $("#monto_dolar").removeAttr("disabled");
                            $('#div_fallecido').hide();
                            $('#div_fallecidomontos').hide();
                            $('#dato_socio').show();
                            $("#calcular").css('visibility', 'visible');
                            $('#cuotas').hide();
                            $('#detalle_aportacion').hide();
                            //$('#efectivo').show();


                        }
                        //venta de divisas
                        else if (valor == 2) {
                            GetTC(valor, 'V');
                            //$("#monto_dolar").prop('disabled', true);
                            $('#dato_deposito').hide();
                            $("#monto_dolar").removeAttr("disabled");
                            $("#monto_cordoba").removeAttr("disabled");
                            $("#prestamo").prop('disabled', true);
                            $('#detalle_prestamo').hide();
                            $('#div_fallecido').hide();
                            $('#div_fallecidomontos').hide();
                            $('#dato_socio').show();
                            $("#calcular").css('visibility', 'visible');
                            $('#cuotas').hide();
                            $('#detalle_aportacion').hide();


                        }
                        //reembolso de divisas compras
                        else if (valor == 3) {
                            GetTC(valor, 'C');
                            $("#monto_dolar").prop('disabled', true);
                            $('#dato_deposito').hide();
                            $("#monto_dolar").val("0");
                            $("#cambio").val("0");
                            $("#prestamo").prop('disabled', true);
                            $('#detalle_prestamo').hide();
                            $("#monto_cordoba").removeAttr("disabled");
                            $('#div_fallecido').hide();
                            $('#div_fallecidomontos').hide();
                            $('#dato_socio').hide();
                            $("#calcular").css('visibility', 'hidden');
                            $('#cuotas').hide();
                            $('#detalle_aportacion').hide();


                        }
                        //reembolso de divisas ventas
                        else if (valor == 5) {
