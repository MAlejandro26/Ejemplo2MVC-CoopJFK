@model CoopJFK.Models.PROFESION

    @{
        ViewBag.Title = "Borrar Profesion";
    }

    <div class="row bg-title">
        <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h4 class="page-title"></h4>
        </div>
        <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
            <ol class="breadcrumb">
                <li><a href="#">Sistema de Gestión de Crédito</a></li>
                <li>Catalogos</li>
                <li class="active">Detalle Profesion</li>
            </ol>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="white-box">
                    <h3>Esta seguro de Borra esta profesión?</h3>
                    <div>
                        <h4>Profesión</h4>
                        <hr />
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayName("Descripción")
                            </dt>
                            <dd>
                                @Html.DisplayFor(model => model.DESCRIPCION)
                            </dd>


                        </dl>

                        @using (Html.BeginForm())
                        {

                            <div class="form-actions no-color">
                                <input type="submit" value="Delete" style="padding:5px 15px" class="btn btn-info" /> |
                                @Html.ActionLink("Atrás", "Index", null, new { @class = "btn btn-primary" })
                            </div>
                        }
                    </div>

                </div>
            </div>
        </div>
    </div>

                   